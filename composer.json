{
    "name": "laahudra/twitch-api-php",
    "type": "library",
    "description": "A Twitch API client for PHP.",
    "keywords": ["twitch", "twitch.tv", "twitch-tv", "twitch-api", "api"],
    "homepage": "http://github.com/laahudra/twitch-api-php",
    "license": "MIT",	  
    "repositories": [  
        {  
            "type": "vcs",  
            "url": "https://github.com/nicklaw5/twitch-api-php"  
        }  
    ], 
    "authors": [
        {
            "name": "Nicholas Law",
            "homepage": "https://github.com/nicklaw5"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "nicklaw5/twitch-api-php": "0.1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "TwitchApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TwitchApi\\Tests\\": "test/"
        }
    }
}
